# -*- mode: ini-generic -*-

root = true

[*]
indent_style = tab
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[package.json]
indent_style = space
indent_size = 2

[*.yml]
indent_style = space
indent_size = 2

[.ebextensions/*.config]
indent_style = space
indent_size = 2
